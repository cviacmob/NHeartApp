<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="1000dp"
    android:layout_height="100dp"

    android:orientation="horizontal"
    >





    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageViewcustom1"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        />



        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView51"
            android:textColor="#FFFFFF"
            android:textSize="12.5dp"
            android:paddingRight="30dp"
            android:layout_toRightOf="@+id/imageViewcustom1"
            android:layout_below="@+id/actionbarTitle1"
           />

        <TextView
            android:text="TextView"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/actionbarTitle1"
            android:textColor="#FFFFFF"
            android:layout_toRightOf="@+id/imageViewcustom1"
             />



    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/ivcall1"
        android:layout_marginLeft="111dp"
        android:layout_marginStart="111dp"
        android:layout_alignTop="@+id/imageViewcustom"
        android:layout_toRightOf="@+id/imageViewcustom"
        android:layout_toEndOf="@+id/relativeLayout" />

</RelativeLayout>